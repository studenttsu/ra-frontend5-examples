{
    "extends": [
        "eslint:recommended"
    ],
    "env": {
        "es2021": true,
        "browser": true
    },
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-async-promise-executor": "off",
        "indent": ["error", 4, {"SwitchCase": 1}],
        "quotes": ["error", "single"]
    }
}
